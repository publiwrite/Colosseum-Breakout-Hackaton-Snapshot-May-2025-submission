  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@libs/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@libs/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@lib/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/db/main/seed/index.ts"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
