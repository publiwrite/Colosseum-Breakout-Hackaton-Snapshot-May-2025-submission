  "scripts": {
    "start": "npm run startLocally",
    "initialize": "./scripts/initialize.sh",
    "dependencies:check-updates": "npm-check-updates",
    "dependencies:update": "npm-check-updates -u",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:user-management": "nest start user-management",
    "start:user-management:dev": "nest start --watch user-management",
    "start:graphql-gateway": "nest start graphql-gateway",
    "start:graphql-gateway:dev": "nest start --watch graphql-gateway",
    "start:graphql-gateway:nodemon": " nodemon --config nodemon.json",
    "start:graphql-gateway:debug": "nest start --debug --watch graphql-gateway",
    "start:checkout-service": "nest start checkout-service",
    "start:checkout-service:dev": "nest start --watch checkout-service",
    "start:book-service": "nest start book-service",
    "start:book-service:dev": "nest start --watch book-service",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/authentication-service/test/jest-e2e.json",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "prisma:seed": "npx prisma db seed",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:studio:main": "npx prisma studio --schema ./prisma/db/main/schema.prisma",
    "prisma:generate-sql:main": "npx prisma generate --schema ./prisma/db/main/schema.prisma && (cd ./prisma/db/main/dbml && dbml2sql schema.dbml -o schema.sql)",
    "db:dev:set-up": "npx prisma db push --force-reset && npx prisma generate && npx prisma db seed",
    "wallet-cli": "ts-node apps/wallets-service/srfc/wallet-cli/main.ts import",
    "helio-webhook": "ngrok http 4010"
  },
